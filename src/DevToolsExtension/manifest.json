{
    "manifest_version": 2,
    "name": "Content Editable Tree View",
    "version": "1.0",
    "permissions": ["devtools"],
    "devtools_page": "devtools.html",
    "web_accessible_resources": ["images/*"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
  